{
  "name": "anahkiasen/underscore-php",
  "description": "A redacted port of Underscore.js for PHP",
  "license": "MIT",
  "keywords": [
    "toolkit",
    "internals",
    "laravel"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    },
    {
      "name": "Marios Vasiliou",
      "email": "mariosv@softline.com.cy"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "doctrine/inflector": "^1.3",
    "patchwork/utf8": "^1.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "fabpot/php-cs-fixer": "2.x",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Underscore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Underscore\\": "tests"
    }
  },
  "scripts": {
    "test": ["php-cs-fixer fix --dry-run", "phpunit --verbose"],
    "lint": "php-cs-fixer fix"
  }
}
